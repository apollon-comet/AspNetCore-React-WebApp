{
    "$schema": "https://json.schemastore.org/package",
    "name": "dsx-client-template",
    "version": "1.0.0",
    "private": true,
    "author": "Devices Software Experiences (DSX)",
    "description": "Web application template",
    "repository": {
        "url": "https://github.com/microsoft/AspNetCore-React-WebApp",
        "type": "git"
    },
    "dependencies": {
        "@fluentui/react": "8.67.4",
        "@uifabric/fluent-theme": "7.6.27",
        "camelcase": "6.3.0",
        "dotenv": "10.0.0",
        "dotenv-expand": "5.1.0",
        "fs-extra": "10.1.0",
        "react": "18.1.0",
        "react-adal": "0.5.1",
        "react-app-polyfill": "3.0.0",
        "react-dom": "18.1.0",
        "react-redux": "8.0.1",
        "react-router-dom": "6.3.0",
        "source-map-loader": "3.0.1",
        "webpack-manifest-plugin": "5.0.0"
    },
    "scripts": {
        "start": "node scripts/start.js",
        "build": "node scripts/build.js",
        "test": "node scripts/test.js"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "jest": {
        "moduleDirectories": [
            "node_modules",
            "src"
        ],
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/**/*.d.ts"
        ],
        "setupFiles": [
            "react-app-polyfill/jsdom"
        ],
        "setupFilesAfterEnv": [],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
            "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
        ],
        "testEnvironment": "jest-environment-jsdom-fourteen",
        "transform": {
            "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
            "^.+\\.module\\.(css|sass|scss)$"
        ],
        "modulePaths": [],
        "moduleNameMapper": {
            "^react-native$": "react-native-web",
            "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
            "office-ui-fabric-react/lib/(.*)$": "office-ui-fabric-react/lib-commonjs/$1"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "web.ts",
            "ts",
            "web.tsx",
            "tsx",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ],
        "watchPlugins": [
            "jest-watch-typeahead/filename",
            "jest-watch-typeahead/testname"
        ]
    },
    "babel": {
        "presets": [
            "react-app"
        ]
    },
    "devDependencies": {
        "@babel/core": "7.18.0",
        "@pmmmwh/react-refresh-webpack-plugin": "0.5.6",
        "@svgr/webpack": "6.2.1",
        "@testing-library/react": "^13.2.0",
        "@types/jest": "27.5.1",
        "@types/node": "17.0.35",
        "@types/node-fetch": "^2.6.1",
        "@types/react": "18.0.9",
        "@types/react-adal": "^0.5.0",
        "@types/react-dom": "18.0.3",
        "@types/react-redux": "^7.1.24",
        "@types/react-router-dom": "^5.3.3",
        "@typescript-eslint/eslint-plugin": "5.25.0",
        "@typescript-eslint/parser": "5.25.0",
        "babel-eslint": "10.1.0",
        "babel-jest": "^28.1.0",
        "babel-loader": "8.2.5",
        "babel-plugin-named-asset-import": "^0.3.8",
        "babel-preset-react-app": "^10.0.1",
        "case-sensitive-paths-webpack-plugin": "2.4.0",
        "css-loader": "^6.7.1",
        "css-minimizer-webpack-plugin": "^3.4.1",
        "eslint": "^8.15.0",
        "eslint-config-react-app": "^7.0.1",
        "eslint-plugin-flowtype": "8.0.3",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-jsx-a11y": "6.5.1",
        "eslint-plugin-react": "7.29.4",
        "eslint-plugin-react-hooks": "4.5.0",
        "eslint-webpack-plugin": "3.1.1",
        "file-loader": "6.2.0",
        "fork-ts-checker-webpack-plugin": "7.2.11",
        "html-webpack-plugin": "5.5.0",
        "identity-obj-proxy": "3.0.0",
        "is-wsl": "^2.2.0",
        "jest": "28.1.0",
        "jest-environment-jsdom-fourteen": "1.0.1",
        "jest-resolve": "28.1.0",
        "jest-watch-typeahead": "1.1.0",
        "mini-css-extract-plugin": "2.6.0",
        "postcss": "8.4.14",
        "postcss-flexbugs-fixes": "5.0.2",
        "postcss-loader": "7.0.0",
        "postcss-normalize": "10.0.1",
        "postcss-preset-env": "6.7.0",
        "react-dev-utils": "^12.0.1",
        "resolve": "1.22.0",
        "sass": "^1.51.0",
        "sass-loader": "13.0.0",
        "semver": "7.3.7",
        "style-loader": "3.3.1",
        "terser-webpack-plugin": "5.3.1",
        "ts-jest": "^28.0.2",
        "ts-pnp": "1.2.0",
        "typescript": "4.6.4",
        "typings-for-css-modules-loader": "^1.7.0",
        "url": "0.11.0",
        "url-loader": "4.1.1",
        "webpack": "5.72.1",
        "webpack-dev-server": "4.9.0",
        "workbox-webpack-plugin": "6.5.3"
    }
}
